<style>
    @import url('https://fonts.googleapis.com/css2?family=Roboto:wght@400;900&display=swap');
    @import url('https://fonts.googleapis.com/css2?family=Catamaran&family=Roboto:wght@400;900&display=swap');

    @page {
        background: var(--header-color-50);
        margin: 60px 0;
    }
    
    @page :first {
        margin-bottom: 60px;
        margin-top: 0;
    }

    :root {
        /*Colors*/
        --text-color: #000;
        --header-color: #3f4959;
        --header-color-50:rgb(86, 100, 122);;
        --header-light-color: #e8eaed;

        /*Fonts*/
        --main-font: 'Playfair Display', serif;
        --secondary-font: 'system-ui', serif;

        /*Sizes*/
        --font-small-xs: 12px;
        --font-small: 14px;
        --font-medium: 16px;
        --font-large: 18px;
        --font-large-xl: 20px;
        --font-large-xxl: 36px;
        --normal: 400;
        --semi-bold: 700;
        --bold: 900;
        --name-font-size: 33px;
        --separator-border: 3px solid;

        /*Spacing*/
        --page-max-width: 45rem;
        --header-height: 180px;
        --photo-size: 120px;

        --dark-gold-color:rgb(169, 136, 75);
        --gold-color: #daa520;
        --gold-text-color:rgb(204, 165, 68);
    }

    * {
        text-rendering: optimizeLegibility;
        box-sizing: border-box;
    }

    html {
        margin: 0;
        padding: 0;
    }

    body {
        background-color: #fff;
        color: var(--text-color);
        font-family: var(--secondary-font);
        margin: 0;
        padding: 0;
    }

    main {
        max-width: var(--page-max-width);
        margin: 0 auto;
        padding: 0;
        padding-top: 115px;
    }

    ul {
        font-family: var(--secondary-font);
        font-style: normal;
        padding-inline-start: 0 !important;
        margin-block-start: 0 !important;
        margin-block-end: 0 !important;
        padding-left: 20px !important;
    }

    .skills ul, .languages ul, .interestFields ul {
        list-style: none;
        padding-left: 0 !important;
    }

    a {
        font-family: var(--secondary-font);
        font-style: normal;
        font-size: var(--font-small);
        line-height: var(--font-medium);
        text-decoration: none !important;
        color: var(--text-color);
    }

    a:active a:focus {
        color: var(--text-color) !important;
    }

    a:hover {
        cursor: pointer;
    }


    /* HEADER */
    .header {
        display: flex;
        flex-direction: column;
        background: var(--header-color);
        margin: 0;
        min-height: 200px;
        width: 100%;
        padding-top: 58px;
        padding-bottom: 10px;
    }

    .about {
        margin-top: 20px;
    }

    .photo {
        position: relative;
        height: 120px;
    }

    .name {
        text-align: center;
        color: white;
        font-family: var(--main-font);
        font-size: var(--font-large-xxl);
        letter-spacing: .2em;
        text-transform: uppercase;
    }

    .job-position {
        font-size:  var(--font-large);
        letter-spacing: .2em;
        margin-top: 18px;
        display: flex;
        align-items: center;
        gap: 22px;
        justify-content: center;
        color: var(--dark-gold-color);
    }

    .job-position-label{
        text-transform: uppercase;
        font-family: var(--main-font);
    }

    .job-position-line {
        background: var(--dark-gold-color);
        height: 2px;
        width: 130px;
    }

    .contact-container {
        margin-top: 10px;
        display: flex;
        justify-content: center;
        align-items: center;
        flex-direction: column;
        flex-wrap: wrap;
        align-content: center;
    }

    .contact{
        display: grid;
        row-gap: 8px;
        column-gap: 32px;
        grid-template-columns: repeat(3, 1fr);
        margin-top: 15px;
        max-width: 578px;
    }

    .item {
        font-family: "Lato";
        font-style: normal;
        font-size: 19px;
        font-weight: 500;
        display: flex;
        flex-direction: row;
        align-items: flex-start;
        margin-bottom: 10px;
        page-break-inside: avoid;
        break-inside: avoid;
    }

    .contact .item svg {
        width: 15px;
        height: 15px;
        margin-right: 11px;
        margin-top: 2px;
        flex-shrink: 0;
    }

    .value {
        font-family: var(--secondary-font);
        font-style: normal;
        font-weight: 200;
        font-size: 11px;
        line-height: 18px;
        color: white;
    }

    .location-wrapper {
        display: flex;
        flex-direction: column;
    }

    .city-underline {
        text-decoration: underline;
    }

    .profile-container {
        display: flex;
        gap: 30px;
        flex-direction: row;
        padding-block: 30px;
        padding-inline: 24px;
        margin-top: -60px;
       
    }

    .profile-description {
        padding-block: 15px;
        align-self: center;
        font-family: var(--secondary-font);
        font-weight: 100;
        font-size: 14px;
        color: #5f5f5f;
    }

    .top-container-image {
        display: flex;
        justify-content: center;
        align-items: flex-start;
    }

    .base64imageRound {
        margin: auto;
        width: 125px;
        height: 125px;
        border-radius: 50%;
        object-fit: cover !important;
    }

    .content-container {
        display: flex;
        flex-direction: row;
        margin-inline: 0;
    }

    .content-container > :nth-child(1) {
        flex: 1;
        padding-left: 0;
    }

    .vertical-separator:after {
        content: "";
        margin-inline: auto;
        display: block;
        width: 1px;
        height: 100%;
        background: #000;
    }

    aside {
        background: var(--header-light-color);
        padding: 20px 90px 80px 0px;
        margin-left: calc(-50vw + 50%);
        padding-left: calc(50vw - 50%);
        position: relative;
    }
    
    aside::after {
        content: "";
        position: absolute;
        right: 0;
        top: 0;
        bottom: 0;
        width: 1px;
        background: #000;
    }
    
    aside>div {
        margin-left: 30px;
    }
    
    aside>div:first-child {
        padding-top: 0;
    }
    
    aside>div:last-child {
        padding-bottom: 20px;
    }
    
    .content-container > :nth-child(2) {
        flex: 0 0 0px;
        display: none;
    }
    
    .content-container > :nth-child(3) {
        flex: 1.6;
        padding-left: 90px;
        padding-right: 12px;
    }

    aside>div:not(:last-child) {
        margin-bottom: 45px;
    }
    
    aside>div {
        page-break-inside: avoid;
        break-inside: avoid;
    }

    .section-title {
        font-family: var(--main-font);
        font-size: 17px;
        margin-bottom: 24px;
        letter-spacing: 2.7px;
        text-transform: uppercase;
        page-break-after: avoid;
        page-break-inside: avoid;
        break-inside: avoid;
    }

    .section-title:after {
        margin-top: 20.5px;
        content: "";
        display: block;
        width: 100px;
        height: .8px;
        background: #191919;
    }

    .experiences .element .position {
        font-family: var(--main-font);
        font-size: 14px;
        line-height: 1.64;
        letter-spacing: 2px;
        text-transform: uppercase;
        color: var(--dark-gold-color);
    }

    .educations .element .degree {
        font-family: var(--main-font);
        font-size: 14px;
        line-height: 1.64;
        letter-spacing: 2px;
        text-transform: uppercase;
        color: var(--gold-color);
    }
    
    .skillsTitle {
        font-family: var(--main-font);
        font-size: 14px;
        line-height: 1.64;
        letter-spacing: 2px;
        text-transform: uppercase;
        margin-bottom: 8px;
        color: var(--gold-color);
    }


    .experiences .element .dates,
    .educations .element .dates,
    .schooladdress {
        font-family: var(--secondary-font);
        font-style: italic;
        font-weight: 100;
        font-size: 14px;
        line-height: 20px;
        text-transform: capitalize;
        color: #343537;
    }

    .interestFields .item {
        font-family: var(--secondary-font);
        font-weight: 200;
        font-size: 12px;
        line-height: 24px;
    }

    .info {
        display: flex;
        flex-direction: row;
        justify-content: space-between;
    }

    .element {
        margin-bottom: 34px;
    }

    .experiences .element .description,
    .educations .element .description,
    .skills-value {
        font-family: var(--secondary-font);
        font-weight: 200;
        font-size: 12px;
        line-height: 1.75;
        margin-top: 15px;
    }

    .technicalSkills,
    .softSkills {
        margin-bottom: 15px;
        page-break-inside: avoid;
        break-inside: avoid;
    }

    .skills {
        padding-top: 0;
    }
    
    .languages {
        padding-top: 20px;
    }

    .skills-value {
        line-height: 0.8;
        margin-bottom: -3px;
        page-break-inside: avoid;
        break-inside: avoid;
    }

    .languages .item {
        flex-direction: column;
        align-items: normal;
        .value {
            color: black;

        }
    }

    .highlight {
        font-weight: 300;
    }

    .highlight mark, .highlight strong.highlight-text {
        color: var(--dark-gold-color);
        background: none;
        font-weight: 600;
    }

    .uppercase {
        text-transform: uppercase;
    }

    .justify {
        text-align: justify  ;
    }
</style>